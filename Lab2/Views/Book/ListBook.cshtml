
@{
    ViewBag.Title = "ListBook";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <ul>
            @{ int i = 1;}
            @foreach (var item in ViewBag.Books) 
            {
                <li>@i -<b>@item</b></li>
                i++;
            }
        </ul>
    </div>

</body>
</html>